[codespell]
skip = .git,*.pdf,*.svg,*.html,dataset_description.json,*.gii
# te - TE
# Weill - name
# reson - Reson. abbreviation in citation
# DNE: acronym for does not exist
# fo: acronym for file object
# Gage - name
# nd - package
# pres - variable
# fixel - real word
# assertIn - function
ignore-words-list = te,weill,reson,DNE,fo,Gage,nd,pres,fixel,assertIn
